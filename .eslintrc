{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "overrides": [
    {
      "files": ["*.ts"],
      "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
      ],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      },
      "rules": {
        "import/prefer-default-export": "off"
      }
    }
  ]
}
